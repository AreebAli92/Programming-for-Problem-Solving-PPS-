#include<stdio.h>
int main()
{
    int i, j, a, b, c, d, e;
    printf("Enter 1st Integer : ");
    scanf("%d",&i);
    printf("Enter 2nd Integer : ");
    scanf("%d",&j);
    a=i+j;
    b=i-j;
    c=i*j;
    d=i/j;
    e=i%j;
    printf("Addition : %d\n",a);
    printf("Subtraction : %d\n",b);
    printf("product : %d\n",c);
    printf("quotient : %d\n",d);
    printf("remainder : %d\n",e);
}
